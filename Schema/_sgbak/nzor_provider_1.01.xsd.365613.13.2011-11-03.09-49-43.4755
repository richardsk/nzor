<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2011 rel. 3 sp1 (http://www.altova.com) by Nick Spencer (Landcare Research) -->
<xs:schema xmlns="http://www.nzor.org.nz/schema/provider/102" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.nzor.org.nz/schema/provider/102" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="DataSet">
		<xs:annotation>
			<xs:documentation>NZOR data set from Providers</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Metadata">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Provider">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Name" type="xs:string" minOccurs="0"/>
										<xs:element name="OrganisationURL" type="xs:string" minOccurs="0"/>
									</xs:sequence>
									<xs:attribute name="id" type="xs:string" use="required"/>
								</xs:complexType>
							</xs:element>
							<xs:element name="DateGenerated" type="xs:dateTime"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="DataSource" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Usage" type="usageType" minOccurs="0"/>
							<xs:element name="Publications" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Publication" maxOccurs="unbounded">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Citation" type="xs:string"/>
													<xs:element name="Identifiers" minOccurs="0">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="Identifier" maxOccurs="unbounded">
																	<xs:complexType>
																		<xs:simpleContent>
																			<xs:extension base="xs:string">
																				<xs:attribute name="type" use="required">
																					<xs:simpleType>
																						<xs:restriction base="xs:string">
																							<xs:enumeration value="doi"/>
																							<xs:enumeration value="lsid"/>
																							<xs:enumeration value="isbn"/>
																							<xs:enumeration value="sici"/>
																							<xs:enumeration value="other"/>
																						</xs:restriction>
																					</xs:simpleType>
																				</xs:attribute>
																			</xs:extension>
																		</xs:simpleContent>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="Authors" minOccurs="0">
														<xs:complexType>
															<xs:choice>
																<xs:element name="Simple" type="xs:string">
																	<xs:annotation>
																		<xs:documentation>Formatted author string or 1 or more authors.  </xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:sequence>
																	<xs:element name="Author" maxOccurs="unbounded">
																		<xs:annotation>
																			<xs:documentation>Parsed representation of authors. Would be possible to parse further into surname, given names etc.  Format should be standardised - at least within a single file.  May be useful to add an element to the root indicating the format for agents (e.g.,. "Surname, A. I." etc)???</xs:documentation>
																		</xs:annotation>
																		<xs:complexType>
																			<xs:simpleContent>
																				<xs:extension base="xs:string">
																					<xs:attribute name="type">
																						<xs:simpleType>
																							<xs:restriction base="xs:string">
																								<xs:enumeration value="primary"/>
																								<xs:enumeration value="secondary"/>
																							</xs:restriction>
																						</xs:simpleType>
																					</xs:attribute>
																				</xs:extension>
																			</xs:simpleContent>
																		</xs:complexType>
																	</xs:element>
																</xs:sequence>
															</xs:choice>
														</xs:complexType>
													</xs:element>
													<xs:element name="Dates" minOccurs="0">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="Date" minOccurs="0" maxOccurs="unbounded">
																	<xs:annotation>
																		<xs:documentation>string representation of date - not constrained to date type??
The type of date can be modified to indicate this is the actual date of publication, written on the publication etc (may need expanding)</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:simpleContent>
																			<xs:extension base="xs:string">
																				<xs:attribute name="type">
																					<xs:simpleType>
																						<xs:restriction base="xs:string">
																							<xs:enumeration value="of publication"/>
																							<xs:enumeration value="on publication"/>
																						</xs:restriction>
																					</xs:simpleType>
																				</xs:attribute>
																			</xs:extension>
																		</xs:simpleContent>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="Titles">
														<xs:annotation>
															<xs:documentation>Contains any titles for a reference eg., chapter title, book title, series title.  The relative level of each title indicated by @level attribute on the title</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="Title" maxOccurs="unbounded">
																	<xs:complexType>
																		<xs:simpleContent>
																			<xs:extension base="xs:string">
																				<xs:attribute name="level" type="xs:int"/>
																				<xs:attribute name="type">
																					<xs:simpleType>
																						<xs:restriction base="xs:string">
																							<xs:enumeration value="full"/>
																							<xs:enumeration value="abbreviation"/>
																						</xs:restriction>
																					</xs:simpleType>
																				</xs:attribute>
																			</xs:extension>
																		</xs:simpleContent>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="Editors" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Editors of reference either as a simple string, or as series of agent names.  (Could be combined with Authors.)</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:choice>
																<xs:element name="Simple" type="xs:string"/>
																<xs:sequence>
																	<xs:element name="Editor" type="xs:string" maxOccurs="unbounded"/>
																</xs:sequence>
															</xs:choice>
														</xs:complexType>
													</xs:element>
													<xs:element name="Volume" type="xs:string" minOccurs="0"/>
													<xs:element name="Issue" type="xs:string" minOccurs="0"/>
													<xs:element name="Edition" type="xs:string" minOccurs="0"/>
													<xs:element name="Pages" minOccurs="0">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="Page" maxOccurs="unbounded">
																	<xs:complexType>
																		<xs:simpleContent>
																			<xs:extension base="xs:string">
																				<xs:attribute name="type">
																					<xs:simpleType>
																						<xs:restriction base="xs:string">
																							<xs:enumeration value="start"/>
																							<xs:enumeration value="end"/>
																							<xs:enumeration value="total"/>
																							<xs:enumeration value="other"/>
																						</xs:restriction>
																					</xs:simpleType>
																				</xs:attribute>
																			</xs:extension>
																		</xs:simpleContent>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="Publisher" minOccurs="0">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="Name" type="xs:string"/>
																<xs:element name="City" type="xs:string"/>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="Keywords" minOccurs="0">
														<xs:complexType>
															<xs:choice>
																<xs:element name="Simple" type="xs:string"/>
																<xs:sequence>
																	<xs:element name="Keyword" type="xs:string" maxOccurs="unbounded"/>
																</xs:sequence>
															</xs:choice>
														</xs:complexType>
													</xs:element>
													<xs:element name="Links" minOccurs="0">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="Link" maxOccurs="unbounded">
																	<xs:complexType>
																		<xs:simpleContent>
																			<xs:extension base="xs:string">
																				<xs:attribute name="type">
																					<xs:simpleType>
																						<xs:restriction base="xs:string">
																							<xs:enumeration value="url"/>
																						</xs:restriction>
																					</xs:simpleType>
																				</xs:attribute>
																			</xs:extension>
																		</xs:simpleContent>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
												<xs:attribute name="id" type="xs:string" use="required"/>
												<xs:attribute name="parentPublicationRef" type="xs:string"/>
												<xs:attribute name="type" use="required">
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="Book"/>
															<xs:enumeration value="Series"/>
															<xs:enumeration value="Chapter"/>
															<xs:enumeration value="Journal"/>
															<xs:enumeration value="Article"/>
															<xs:enumeration value="Thesis"/>
															<xs:enumeration value="Magazine"/>
															<xs:enumeration value="Report"/>
															<xs:enumeration value="Abstract"/>
															<xs:enumeration value="Pamphlet"/>
															<xs:enumeration value="Electronic source"/>
															<xs:enumeration value="Serial"/>
															<xs:enumeration value="Generic"/>
															<xs:enumeration value="Unpublished"/>
															<xs:enumeration value="Catalog"/>
															<xs:enumeration value="In press"/>
															<xs:enumeration value="Conference proceedings"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="createdDate" type="xs:dateTime">
													<xs:annotation>
														<xs:documentation>Date record created</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute name="modifiedDate" type="xs:dateTime">
													<xs:annotation>
														<xs:documentation>Date record last modified</xs:documentation>
													</xs:annotation>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="TaxonNames" minOccurs="0">
								<xs:complexType>
									<xs:choice maxOccurs="unbounded">
										<xs:element name="TaxonName">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="FullName" type="xs:string">
														<xs:annotation>
															<xs:documentation>The full name including appropriate rank and author abbreviations.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="Rank" type="xs:string"/>
													<xs:element name="CanonicalName" type="xs:string">
														<xs:annotation>
															<xs:documentation>The epithet part of the name.  E.g. for a species it is just the species component not including the Genus.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="Authorship" type="xs:string">
														<xs:annotation>
															<xs:documentation>separate elements for basionym and combination authorship - is this needed for data consumers</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="BasionymAuthors" type="xs:string"/>
													<xs:element name="CombiningAuthors" type="xs:string"/>
													<xs:element name="PublishedIn">
														<xs:complexType>
															<xs:simpleContent>
																<xs:extension base="xs:string">
																	<xs:attribute name="ref" type="xs:string"/>
																</xs:extension>
															</xs:simpleContent>
														</xs:complexType>
													</xs:element>
													<xs:element name="Year" type="xs:string"/>
													<xs:element name="MicroReference" type="xs:string"/>
													<xs:element name="TypeName" type="nameRefType" minOccurs="0"/>
													<xs:element name="Orthography" type="xs:string" minOccurs="0"/>
													<xs:element name="Basionym" type="nameRefType" minOccurs="0"/>
													<xs:element name="LaterHomonymOf" type="nameRefType" minOccurs="0"/>
													<xs:element name="ReplacementFor" minOccurs="0">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="BlockedName" type="nameRefType"/>
																<xs:element name="RecombinedName" type="nameRefType" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Required for comb.nov. only</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="NomenclaturalStatus" type="xs:string" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Is there a business case for this to be more explicit/enumerated?  If so what?</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="NomenclaturalCode" type="xs:string"/>
												</xs:sequence>
												<xs:attribute name="id" type="xs:string" use="required"/>
												<xs:attribute name="createdDate" type="xs:dateTime">
													<xs:annotation>
														<xs:documentation>Date record created</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute name="modifiedDate" type="xs:dateTime">
													<xs:annotation>
														<xs:documentation>Date record last modified</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute name="qualityCode">
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="Null"/>
															<xs:enumeration value="Unvalidated"/>
															<xs:enumeration value="Partially Validated"/>
															<xs:enumeration value="Validated Mixed Sources"/>
															<xs:enumeration value="Validated Secondary Source"/>
															<xs:enumeration value="Validated Primary Source"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
										<xs:element name="VernacularName">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="FullName" type="xs:string"/>
													<xs:element name="PublishedIn" minOccurs="0">
														<xs:complexType>
															<xs:simpleContent>
																<xs:extension base="xs:string">
																	<xs:attribute name="ref" type="xs:string"/>
																</xs:extension>
															</xs:simpleContent>
														</xs:complexType>
													</xs:element>
													<xs:element name="Language" type="xs:string"/>
													<xs:element name="Country" type="xs:string" minOccurs="0">
														<xs:annotation>
															<xs:documentation>the country in which this name is thought to have been derived (vernacular) OR the jurisdication for tradenames.</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
												<xs:attribute name="id" type="xs:string" use="required"/>
												<xs:attribute name="isTradeName" type="xs:boolean"/>
												<xs:attribute name="createdDate" type="xs:dateTime"/>
												<xs:attribute name="modifiedDate" type="xs:dateTime"/>
											</xs:complexType>
										</xs:element>
									</xs:choice>
								</xs:complexType>
							</xs:element>
							<xs:element name="TaxonConcepts" minOccurs="0">
								<xs:complexType>
									<xs:choice maxOccurs="unbounded">
										<xs:element name="TaxonConcept">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="TaxonName">
														<xs:complexType>
															<xs:complexContent>
																<xs:extension base="nameRefType"/>
															</xs:complexContent>
														</xs:complexType>
													</xs:element>
													<xs:element name="AccordingTo">
														<xs:complexType>
															<xs:simpleContent>
																<xs:extension base="xs:string">
																	<xs:attribute name="ref" type="xs:string"/>
																</xs:extension>
															</xs:simpleContent>
														</xs:complexType>
													</xs:element>
													<xs:element name="Rank" type="xs:string" minOccurs="0">
														<xs:annotation>
															<xs:documentation>May be required when a name is used at a rank that difference to its nomenclatural rank (rare)</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="Relationships">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="AcceptedConcept" minOccurs="0">
																	<xs:complexType>
																		<xs:attribute name="ref" type="xs:string" use="required">
																			<xs:annotation>
																				<xs:documentation>Refers to either another TaxonConcept or a NameBasedConcept</xs:documentation>
																			</xs:annotation>
																		</xs:attribute>
																		<xs:attribute name="inUse" type="xs:boolean" use="required"/>
																	</xs:complexType>
																</xs:element>
																<xs:element name="ParentConcept" minOccurs="0">
																	<xs:complexType>
																		<xs:attribute name="ref" type="xs:string" use="required">
																			<xs:annotation>
																				<xs:documentation>Refers to either another TaxonConcept or a NameBasedConcept</xs:documentation>
																			</xs:annotation>
																		</xs:attribute>
																		<xs:attribute name="inUse" type="xs:boolean" use="required"/>
																	</xs:complexType>
																</xs:element>
																<xs:element name="RelatedConcept" minOccurs="0" maxOccurs="unbounded">
																	<xs:annotation>
																		<xs:documentation>required for anamorph/teleomorph; hybrids</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:attribute name="ref" type="xs:string" use="required">
																			<xs:annotation>
																				<xs:documentation>Refers to either another TaxonConcept or a NameBasedConcept</xs:documentation>
																			</xs:annotation>
																		</xs:attribute>
																		<xs:attribute name="type" use="required">
																			<xs:annotation>
																				<xs:documentation>the concept is the *type* for the related concept</xs:documentation>
																			</xs:annotation>
																			<xs:simpleType>
																				<xs:restriction base="xs:string">
																					<xs:enumeration value="is parent of"/>
																					<xs:enumeration value="is vernacular for"/>
																					<xs:enumeration value="is child of"/>
																					<xs:enumeration value="has vernacular of"/>
																					<xs:enumeration value="is anamorph of"/>
																					<xs:enumeration value="is teleomorph of"/>
																					<xs:enumeration value="is hybrid parent for"/>
																					<xs:enumeration value="is hybrid child of"/>
																					<xs:enumeration value="is synonym of"/>
																				</xs:restriction>
																			</xs:simpleType>
																		</xs:attribute>
																		<xs:attribute name="rank" type="xs:string" use="required"/>
																		<xs:attribute name="inUse" type="xs:boolean" use="required"/>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
												<xs:attribute name="id" type="xs:string" use="required"/>
												<xs:attribute name="createdDate" type="xs:dateTime">
													<xs:annotation>
														<xs:documentation>Date record created</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute name="modifiedDate" type="xs:dateTime">
													<xs:annotation>
														<xs:documentation>Date record last modified</xs:documentation>
													</xs:annotation>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
										<xs:element name="VernacularConcept">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="VernacularName">
														<xs:annotation>
															<xs:documentation>The verbatim text as cited in the source reference (AccordingTo).  The attribute ref refers to a VernacularName</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:complexContent>
																<xs:extension base="nameRefType"/>
															</xs:complexContent>
														</xs:complexType>
													</xs:element>
													<xs:element name="AccordingTo">
														<xs:complexType>
															<xs:simpleContent>
																<xs:extension base="xs:string">
																	<xs:attribute name="ref" type="xs:string"/>
																</xs:extension>
															</xs:simpleContent>
														</xs:complexType>
													</xs:element>
													<xs:element name="Rank" type="xs:string" minOccurs="0">
														<xs:annotation>
															<xs:documentation>May be required when a name is used at a rank that difference to its nomenclatural rank (rare)</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="Applications">
														<xs:annotation>
															<xs:documentation>For vernacular useages</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="Application" maxOccurs="unbounded">
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="Gender" type="xs:string" minOccurs="0"/>
																			<xs:element name="PartOfTaxon" type="xs:string" minOccurs="0"/>
																			<xs:element name="LifeStage" type="xs:string" minOccurs="0"/>
																			<xs:element name="GeoRegion" minOccurs="0">
																				<xs:complexType>
																					<xs:simpleContent>
																						<xs:extension base="xs:string">
																							<xs:attribute name="geographicSchema" type="xs:string"/>
																						</xs:extension>
																					</xs:simpleContent>
																				</xs:complexType>
																			</xs:element>
																		</xs:sequence>
																		<xs:attribute name="conceptRef" type="xs:string" use="required">
																			<xs:annotation>
																				<xs:documentation>Reference to a TaxonConcept</xs:documentation>
																			</xs:annotation>
																		</xs:attribute>
																		<xs:attribute name="type">
																			<xs:simpleType>
																				<xs:restriction base="xs:string">
																					<xs:enumeration value="is vernacular for"/>
																					<xs:enumeration value="is trade name for"/>
																				</xs:restriction>
																			</xs:simpleType>
																		</xs:attribute>
																		<xs:attribute name="inUse" type="xs:boolean" use="required"/>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
												<xs:attribute name="id" type="xs:string" use="required"/>
												<xs:attribute name="createdDate" type="xs:dateTime">
													<xs:annotation>
														<xs:documentation>Date record created</xs:documentation>
													</xs:annotation>
												</xs:attribute>
